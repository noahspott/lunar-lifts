---
// Components
import { Image } from "astro:assets";

// Images
import shapeDivider from "@/assets/images/decorative/triangleAsymmetrical.png";

const { className } = Astro.props as { className: string };
---

<Image
  class={`absolute top-0 left-1/2 -translate-x-1/2 ${className}`}
  alt=""
  src={shapeDivider}
  loading="eager"
/>
