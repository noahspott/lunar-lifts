---
// Components
import Layout from "../layouts/Layout.astro";
import {
  Hero,
  ProductHighlight,
  WhyChooseUs,
  FeaturedFaqs,
} from "@/components/home";
import { ContactSection } from "@/components/shared";

// Data
import { seo } from "@/data/seo";
---

<Layout seo={seo.home}>
  <Hero className="relative z-20" />
  <ProductHighlight />
  <WhyChooseUs className="relative z-20" />
  <FeaturedFaqs />
  <ContactSection className="relative z-20" client:load />
</Layout>
